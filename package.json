{
  "name": "vl-ui-form-grid",
  "version": "1.0.6",
  "description": "De vl-form-grid webcomponent maakt heb mogelijk om opmaak te geven aan formulieren",
  "main": "vl-form-grid.js",
  "scripts": {
    "prepare": "npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run util",
    "test": "wct -l chrome,firefox --npm",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:imports && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "uglifyjs --compress --mangle --output vl-form-grid.js -- *.src.js",
    "release:prepare:imports": "replace '/node_modules/vl-ui-core/vl-core.src.js' '/node_modules/vl-ui-core/vl-core.js' vl-form-grid.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-form-grid/style.css' vl-form-grid.js",
    "release:prepare:commit": "git add -f vl-form-grid.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "dependencies": {
    "vl-ui-core": "1.4.3",
    "vl-ui-grid": "1.0.4"
  },
  "peerDependencies": {
    "vl-ui-core": "^1.4.2"
  },
  "devDependencies": {
    "@govflanders/vl-ui-core": "3.7.2",
    "http-server": "0.11.1",
    "np": "3.0.4",
    "replace": "1.1.0",
    "uglify-js-es6": "2.8.9",
    "vl-ui-button": "1.1.3",
    "vl-ui-checkbox": "1.0.7",
    "vl-ui-form-message": "2.1.1",
    "vl-ui-input-field": "1.0.4",
    "vl-ui-textarea": "1.0.4",
    "vl-ui-util": "^1.2.3",
    "wct-browser-legacy": "1.0.2",
    "web-component-tester": "6.9.2"
  },
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-form-grid.git"
  },
  "author": "DIDM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vl-ui-form-grid/issues"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-form-grid#readme"
}
